{
  "name": "solaire",
  "version": "0.0.0",
  "description": "The grossly incandescent procedural generation framework",
  "main": "dist/solaire.js",
  "jsnext:main": "dist/solaire.module.js",
  "module": "dist/solaire.module.js",
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "keywords": [
    "procedural-generation",
    "game-development"
  ],
  "scripts": {
    "lint": "eslint --fix src test",
    "test": "karma start --single-run --browsers ChromeHeadless",
    "clean": "rimraf build dist",
    "build": "rollup --config",
    "start": "serve -l 8080",
    "report-coverage": "cat ./build/coverage/lcov.info | codecov",
    "release": "standard-version",
    "release-dry-run": "standard-version --dry-run"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "engines": {
    "node": "6.x",
    "npm": "3.x"
  },
  "dependencies": {
    "merge": "^1.2.1",
    "seedrandom": "^3.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.6.1",
    "@commitlint/config-conventional": "^7.6.0",
    "babel-core": "^6.26.3",
    "babel-plugin-external-helpers": "^6.18.0",
    "babel-preset-es2015": "^6.24.1",
    "codecov.io": "^0.1.6",
    "eslint": "^4.19.1",
    "eslint-plugin-jasmine": "^2.10.1",
    "husky": "^2.2.0",
    "jasmine": "^3.3.1",
    "jasmine-core": "^3.3.0",
    "karma": "^3.1.4",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-firefox-launcher": "^1.1.0",
    "karma-jasmine": "^1.1.2",
    "karma-rollup-preprocessor": "^5.1.1",
    "karma-safari-launcher": "^1.0.0",
    "karma-spec-reporter": "0.0.32",
    "rimraf": "^2.6.3",
    "rollup": "^0.58.2",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-istanbul": "^2.0.1",
    "rollup-plugin-node-resolve": "^3.4.0",
    "serve": "^10.1.1",
    "standard-version": "^4.4.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jsalis/solaire.git"
  },
  "author": "jsalis <johnsalis@live.com>",
  "license": "MIT"
}

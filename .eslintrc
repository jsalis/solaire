{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "browser": true,
    "commonjs": true
  },
  "plugins": [
    "jasmine"
  ],
  "rules": {
    "arrow-parens": [2, "as-needed"],
    "arrow-spacing": 2,
    "brace-style": 2,
    "curly": [2, "multi-line", "consistent"],
    "dot-location": [2, "property"],
    "eol-last": 2,
    "eqeqeq": [2, "smart"],
    "indent": [2, "tab", { "SwitchCase": 1 }],
    "key-spacing": [2, { "mode": "minimum" }],
    "keyword-spacing": 2,
    "new-cap": 2,
    "no-alert": 2,
    "no-console": 1,
    "no-array-constructor": 2,
    "no-cond-assign": [2, "always"],
    "no-constant-condition": 2,
    "no-debugger": 2,
    "no-div-regex": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-semi": 2,
    "no-implicit-coercion": [2, { "boolean": false }],
    "no-implied-eval": 2,
    "no-iterator": 2,
    "no-lone-blocks": 2,
    "no-multi-spaces": [2, { "exceptions": { "ObjectExpression": true }}],
    "no-multiple-empty-lines": ["error", { "max": 1 }],
    "no-native-reassign": 2,
    "no-negated-condition": 2,
    "no-new-func": 2,
    "no-new-object": 2,
    "no-new-wrappers": 2,
    "no-redeclare": 2,
    "no-script-url": 2,
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-spaced-func": 2,
    "no-throw-literal": 2,
    "no-trailing-spaces": 2,
    "no-undef": 2,
    "no-unneeded-ternary": 2,
    "no-unreachable": 2,
    "no-unused-expressions": 2,
    "no-unused-vars": 2,
    "no-useless-call": 2,
    "no-useless-concat": 2,
    "object-curly-spacing": [2, "always", { "objectsInObjects": false, "arraysInObjects": true }],
    "prefer-arrow-callback": ["error", { "allowNamedFunctions": true }],
    "quote-props": ["error", "as-needed", { "keywords": true, "unnecessary": false }],
    "quotes": [2, "single", { "avoidEscape": true, "allowTemplateLiterals": true }],
    "radix": 2,
    "semi": 2,
    "space-before-blocks": 2,
    "space-before-function-paren": [2, { "anonymous": "always", "named": "never" }],
    "space-in-parens": 2,
    "space-infix-ops": 2,
    "space-unary-ops": [2, { "words": true, "nonwords": false }],
    "template-curly-spacing": ["error", "always"],
    "use-isnan": 2,
    "valid-jsdoc": [0, { "requireReturn": false, "requireReturnDescription": false }],
    "wrap-iife": [2, "inside"],
    "yoda": 2
  },
  "overrides": [{
    "files": ["*.spec.js", "*-mock.js"],
    "env": {
      "jasmine": true
    },
    "rules": {
      "jasmine/missing-expect": 1,
      "jasmine/named-spy": 2,
      "jasmine/no-disabled-tests": 1,
      "jasmine/no-expect-in-setup-teardown": 1,
      "jasmine/no-focused-tests": 2,
      "jasmine/no-global-setup": 2,
      "jasmine/no-spec-dupes": [1, "branch"],
      "jasmine/no-suite-dupes": [1, "branch"],
      "jasmine/no-suite-callback-args": 2,
      "jasmine/valid-expect": 2
    }
  }]
}